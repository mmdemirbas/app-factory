CREATE TABLE ConfiguredConnector (
    id TEXT NOT NULL PRIMARY KEY,
    team_id TEXT NOT NULL,
    descriptor_id TEXT NOT NULL,
    config_json TEXT NOT NULL,
    updated_at INTEGER NOT NULL
);

selectAll:
SELECT * FROM ConfiguredConnector WHERE team_id = ? ORDER BY updated_at DESC;

selectById:
SELECT * FROM ConfiguredConnector WHERE id = ? AND team_id = ?;

upsert:
INSERT OR REPLACE INTO ConfiguredConnector(id, team_id, descriptor_id, config_json, updated_at)
VALUES (?, ?, ?, ?, ?);

deleteById:
DELETE FROM ConfiguredConnector WHERE id = ? AND team_id = ?;
